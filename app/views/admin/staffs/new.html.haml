%h1 adminログイン画面
= form_with model:@staff,url:admin_session_path, method: :post ,local:true do |f|
  .form-group
    = f.label :name
    = f.text_field :name, class:"form-control"
    = f.label :password
    = f.password_field :password_confirmation, class:"form-control"
    = f.submit "送信する", data: { disable_with: "送信中..." } ,class:"btn btn-primary"

